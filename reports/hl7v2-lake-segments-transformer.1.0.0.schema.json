{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HL7 v2 Message Schema",
  "type": "object",
  "properties": {
    "content_schema_name": {
      "const": "DEX HL7v2 Lake of Segments"
    },
    "content_schema_version": {
      "type": "string",
      "const": "2.0.0"
    },

    "configs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["content_schema_name", "content_schema_version"]
}
